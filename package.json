{
  "name": "eadens-cake-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:prisma dev:next",
    "build": "PRISMA_CLI_BINARY_TARGET=native prisma generate --schema ./eadens-cake-world/prisma/schema.prisma && prisma migrate deploy --schema ./eadens-cake-world/prisma/schema.prisma && next build",
